page {
  theme {
    language.enable = 0
    googleFont.enable = 0
    cookieconsent.enable = 0
    breadcrumb.enableLevel = 1
    copyright {
      enable = true
      text = &copy; 2024 Leseohren e.V.
    }
  }
  logo {
    file = EXT:leseohren/Resources/Public/Icons/logo_leseohren.svg
    fileInverted = EXT:leseohren/Resources/Public/Icons/logo_leseohren-inverted.svg
    height = 60
    width = 168
    alt = Leseohren e.V.
    linktitle = Leseohren e.V.
  }
}

pageIDs {
  personShowPid = 12
  organizationShowPid = 19
  eventShowPid = 225
  blackboardShowPid = 228
}

plugin.tx_leseohren_personen {
    view {
        # cat=plugin.tx_leseohren_personen/file; type=string; label=Path to template root (FE)
        templateRootPath = EXT:leseohren/Resources/Private/Templates/
        # cat=plugin.tx_leseohren_personen/file; type=string; label=Path to template partials (FE)
        partialRootPath = EXT:leseohren/Resources/Private/Partials/
        # cat=plugin.tx_leseohren_personen/file; type=string; label=Path to template layouts (FE)
        layoutRootPath = EXT:leseohren/Resources/Private/Layouts/
    }
    persistence {
        # cat=plugin.tx_leseohren_personen//a; type=string; label=Default storage PID
        storagePid = 13
        blackboardsStoragePid = 13
        giftsStoragePid = 227
    }
}
